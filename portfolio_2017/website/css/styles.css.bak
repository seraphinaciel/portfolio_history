.navicon {
  -webkit-transition: linear 0.5s all;
  transition: linear 0.5s all;
  font-size:0;
}
.navicon:before, .navicon:after {
  background: #333;
  -webkit-backface-visibility: hidden;
          backface-visibility: hidden;
  content: "";
  height: 2px;
  right: 20px;
  -webkit-transition: 0.8s ease;
  transition: 0.8s ease;
  width: 25px;
}
.navicon:before {
  box-shadow: #333 0 10px 0 0;
  position: absolute;
  top: 15px;
}
.navicon:after {
  position: absolute;
  top: 35px;
}
.navicon--active {
  -webkit-transition: linear 0.5s all;
  transition: linear 0.5s all;
}
.navicon--active:before , .navicon--active:after{top: 25px;}
.navicon--active:before {
	box-shadow: transparent 0 0 0 0;
	-webkit-transform: rotate(225deg);
		transform: rotate(225deg);
}
.navicon--active:after {
	-webkit-transform: rotate(315deg);
		transform: rotate(315deg);
}

/* pc menu icon */
@media only screen and (min-width: 768px) {
/* remove mobile icon */
.navicon:before {box-shadow: none;}
.navicon:before, .navicon:after {
  background: none;
  height: auto;
}

	.cd-menu-trigger {
	  display: inline-block;
	  position: absolute;
		top: 16px;
		right: 10px;
		font-size: 16px;
		font-size: 1rem;
	  height: 50px;
	  line-height: 50px;
	  padding: 0 .8em;
	  text-transform: uppercase;
	  font-family: 'Days One', sans-serif;
	  font-weight:bold;
	  text-decoration:none;
		z-index:9999
	}
	.cd-menu-trigger span {
	  /* hamburger icon */
	  position: relative;
	  display: inline-block;
	  width: 18px;
	  height: 2px;
	  background-color: black;
	  vertical-align: middle;
	  margin-left: 10px;
	  -webkit-transform: translateY(-2px);
	  -moz-transform: translateY(-2px);
	  -ms-transform: translateY(-2px);
	  -o-transform: translateY(-2px);
	  transform: translateY(-2px);
	}
	.cd-menu-trigger span::before, .cd-menu-trigger span::after {
	  content: '';
	  display: inline-block;
	  position: absolute;
	  left: 0;
	  width: 80%;
	  height: 100%;
	  background-color: inherit;
	  -webkit-transition: all 0.2s;
	  -moz-transition: all 0.2s;
	  transition: all 0.2s;
	}
	.cd-menu-trigger span::before {
	  top: -6px;
	}
	.cd-menu-trigger span::after {
	  bottom: -6px;
	}
	.no-touch .cd-menu-trigger:hover span::before {
	  top: -8px;
	}
	.no-touch .cd-menu-trigger:hover span::after {
	  bottom: -8px;
	}
}

@media only screen and (min-width: 1170px) {
  .cd-menu-trigger {
    right: 60px;
  }
}
.toggle {
	height:50px;
  opacity: 0;
  -webkit-transition: ease-in 0.5s all;
  transition: ease-in 0.5s all;
  -webkit-transform: translateY(-850%);
          transform: translateY(-850%);
}
.toggle--active {
  display: block;
  position:relative;
  top:34px;
  width:100%;
  z-index:2;
  opacity: 1;
  -webkit-transition: ease-in 0.5s all;
  transition: ease-in 0.5s all;
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
.toggle ul {
    position: absolute;
    width: 100%;
    padding: 50px 15px;
    background: rgba(255,255,255,.9);
}
.toggle li {
	padding:15px 5px;
}
.toggle li a {
  display: block;
  border-bottom: 1px solid rgba(155, 181, 217, .5);
  color: #3a393f;
  font-size: 20px;
  font-size: 1.25rem;
  font-weight: bold;
}
.toggle li a span {
  -webkit-transform: translateX(-50%) translateY(-50%);
  -moz-transform: translateX(-50%) translateY(-50%);
  -ms-transform: translateX(-50%) translateY(-50%);
  -o-transform: translateX(-50%) translateY(-50%);
  transform: translateX(-50%) translateY(-50%);
}
.toggle li:hover {background-color: #f5f5f5;}
.toggle li:last-child a {border-bottom: none;}

@media only screen and (min-width: 768px) {
	.toggle {
		height:80px;
    -webkit-transform: translateX(580%);
    transform: translateX(580%);	
	-webkit-transition: ease 1s;
	transition: ease 1s;
	}
	.toggle--active {
		top:0;
    -webkit-transform: translateX(0%);
    transform: translateX(0%);	
	}
	.toggle ul {top:30px; right:11%; width:auto; padding:0; background:none;}
	.toggle li {padding:5px 10px; display:inline-block;}
	.toggle li a {font-size:1rem; border-bottom:none;}
}
@media only screen and (min-width: 1024px) {
	.toggle ul {tright:19%; }
}


