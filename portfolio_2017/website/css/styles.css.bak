.cardOverlay {
  padding:0;
  margin:20px 10px;
  box-shadow:0 4px 10px 0 rgba(0,0,0,0.2), 0 4px 20px 0 rgba(0,0,0,0.19);
  overflow:hidden;
}

.cardOverlay .image {
  display: block;
  width: 100%;
  height: auto;
    -webkit-transition: opacity 0.35s, -webkit-transform 0.35s;
	transition: opacity 0.35s, transform 0.35s;
}

.overlay {
  position: absolute;
  top: 0;
  height: 100%;
  width: 100%;
  opacity: 0;
  white-space: nowrap;
  transition: .5s ease;
  background-color: rgba(82, 149, 208, .9);
  outline: 1px solid rgba(82, 149, 208, .9);
}

.overlay .text {
	padding:30px;
	width:100%;
	height:100%;
}

.text span{color:#fff; font-size:30px;}

.text div{
	position:absolute; bottom:30px; right:30px; 
	text-align:right; 	
	width:60%;
	}
.text span, .text div * {
	-webkit-transition: -webkit-transform 0.35s;
	transition: transform 0.35s;
	-webkit-transform: scale3d(0.8,0.8,1);
	transform: scale3d(0.8,0.8,1);
	-webkit-transform-origin: 50% 100%;
	transform-origin: 50% 100%;
}

.text h1{color:#ccc; font-weight:bold; text-transform:uppercase}
.text h2{color:#ffc814; padding:10px 0 5px; line-height:20px;}
	
.text p {text-align:left;}
.text p:before {
    position: absolute;
    right: 0;
    top: 0;
    height: 1px;
    width: 100%;
    content: "";
    background-color: #fff;
}
.text p i{color:#fff; font-size:13px; font-weight:100}
.cardOverlay a {display:block; width:100%; height:100%; position:absolute; top:0; left:0; font-size:0;}

.cardOverlay:hover .text span, .cardOverlay:hover .text div * {
	opacity: 1;
	-webkit-transform: scale3d(1,1,1);
	transform: scale3d(1,1,1);
}	

.cardOverlay:hover .image{

	-webkit-transform: scale3d(1.5,1.5,1);
	transform: scale3d(1.5,1.5,1);
}
.cardOverlay:hover .overlay {
  opacity: 1;
}

