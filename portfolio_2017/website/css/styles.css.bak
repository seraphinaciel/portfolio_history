.modalOpen {
  overflow: hidden;
}
.modalOpen .modal {
  overflow-x: hidden;
  overflow-y: auto;
}

.fade {background-color:rgba(255, 255, 255, .8);}
.modal-dialog {
	position: relative;
	width: auto;
	margin: 10px auto;
}
@media (min-width: 768px) {
	.modal-dialog {
		max-width: 1200px;
		margin: 30px auto;
	}
}
.modal-content {border:0; padding-bottom:10px;}
.modal-header {display:grid;}
