/* common */
a  {text-decoration:none;}
body {background:url('../images/main/brick_bg.gif')}
section .container {width:100%;  margin:0 auto; padding:1rem;}
@media only screen and (min-width: 768px){
	section .container {max-width:1200px;}
}

/* headline */
.headline {position:relative;}
.headline .head_bg01 { background: url("../images/main/head_bg01.png") no-repeat center;}
.headline .head_bg02 { background: url("../images/main/head_bg02.png") no-repeat center;}
.headline .head { background-size:contain; max-width:340px; padding:12px 0; margin:0px auto; font:37px 'Amaranth'; text-align:center; position:relative;}
.headline  sup {font:600 13px 'Syncopate'; position:absolute; top:-20px; left:15px; text-align:left;}
@media only screen and (max-width: 340px){
	.headline .head_bg01, .headline .head_bg02 {}
}

/* selection */
	::selection {
  color: #fff;
  background: #f4d260;
  text-shadow: none;
}

::-webkit-selection {
  color: #fff;
  background: #f4d260;
  text-shadow: none;
}

::-moz-selection {
  color: #fff;
  background: #f4d260;
  text-shadow: none;
}

/* button */
.button_box {text-align:center; padding:30px 0}
.button_box p {padding:0;}
.button {
	padding: 15px 0;
	width:100%;
	overflow: hidden;
	font-weight:400;
	border: none;
	outline: none;
	cursor: pointer;
	display: inline-block;
	text-transform: uppercase;
	position: relative;
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	transition: all 0.3s;
}
.btn_seework {	
	color: #5295d0;
	background: #ccc;
}
.btn_downlode {
	background: #5295d0;
	color: #eee;
}
.btn_downlode:before{
	content: '';
	font-size:1rem;
	color: #fff;
	speak: none;
	text-transform: uppercase;
	position: relative;
	-webkit-font-smoothing: antialiased;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	line-height: 54px;
	-webkit-backface-visibility: hidden;
	-moz-backface-visibility: hidden;
	backface-visibility: hidden;
	z-index: 1;
}
.btn_downlode:after {
	content: '';
	background: rgba(0, 123, 193, .5);
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	transition: all 0.3s;
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
	z-index: 0;
	width: 0;
}
.btn_downlode.btn-activated:after {
	-webkit-animation: fillToRight 0.7s forwards;
	-moz-animation: fillToRight 0.7s forwards;
	animation: fillToRight 0.7s forwards;
}
@-webkit-keyframes fillToRight { to { width: 100%;} }
@-moz-keyframes fillToRight { to { width: 100%;} }
@keyframes fillToRight { to { width: 100%;	} }

/* ΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞ */
/* 【header/nav】 */
/* ΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞΞ */
  .affix {
