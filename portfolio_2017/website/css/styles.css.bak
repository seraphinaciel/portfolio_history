	.modal-header p {font-size:15px; color:#8a959c;}
.modal-body {
    position: relative;
    padding: 0;
}	
.modal-body iframe {height:100vmin;}

.modal-footer {border-top:1px solid #828282; border-bottom:1px solid #828282; margin: 10px; text-align: center;}

/* icon */
.modal-content span.fa {cursor:pointer; color:#828282; vertical-align: middle; font-family:FontAwesome;}
.modal-content span.fa:hover {opacity:.8;}
.icon_close {
	position: absolute;
	top: 20px;
	right: 20px;
	font-size: 48px;
}
.icon_th {margin: 0 3rem; font-size: 1.5rem;}

