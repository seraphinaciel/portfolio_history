	#view_contents #worknote h1 {font-size:1.5em; font-weight:900; padding: 31px 0 57px;}
	#view_contents #worknote h1 em {color:#333; font-family:'NanumSquareR', 'nanumgothic'; padding-bottom: 5px;}
	#view_contents #worknote p {color:#888; line-height: 20px;}
	#view_contents #worknote p span {display:block; text-align:right; font-size:13px;}
#view_contents #detail {border-left: 1px solid;}
#view_contents #detail ul {display:inline-block; vertical-align:top; padding-top: 28px;}
	#view_contents #detail ul.right {padding-left:1rem; color:#888;}
	#view_contents #detail ul br {display:none;}
@media (max-device-width: 1024px) and (min-device-width: 768px) {
	#view_contents #detail ul br {display:initial;}
}
.viewmore{
	outline:none;
	font-family: inherit;
	font-size: inherit;
	background: none;
	cursor: pointer;
	padding: 10px 50px;
	display: inline-block;
	margin: 15px auto;
	text-transform: uppercase;
	letter-spacing: 1px;
	font: 600 14px 'arial';
	outline: none;
	position: relative;
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	transition: all 0.3s;
	border-radius: 10px;
	border: 2px solid #299ed9;
	color: #299ed9;
	overflow: hidden;
}

.viewmore:after {
	content: '';
	position: absolute;
	z-index: -1;
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	transition: all 0.3s;
}

.viewmore:before {
	content: "→";
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	-webkit-font-smoothing: antialiased;
	position: absolute;
	left: 130%;
	top: 0;
	height: 100%;
	font-size: 160%;
	line-height: 1.5;
	color: #fff;
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	transition: all 0.3s;
}

.viewmore:active {
	background-color: #008ccf;
	color: #008ccf;
	outline: none;
}
.viewmore:active:before {
	color: #fff;
}

.viewmore:hover {
	background: #299ed9;
	color:#fff;
	outline: none;
}
.viewmore:hover:before {
	left: 80%;
}

