#whatIDo .doBox .col-xs-6 {width:48%; margin-right:10px; border-bottom:5px solid #333; box-shadow:5px 5px 5px rgba(204,204,204,.3);}
	/* icon hover effect */
	.icon_wrap {text-align: center; margin: 0 auto;}
	.ficon {
		display: inline-block;
		font-size: 0px;
		margin: 30px auto;
		width: 40px;
		height: 40px;
		text-align: center;
		position: relative;
		z-index: 1;
		color: #fff;
		vertical-align:middle;
	}
	.ficon:after {
		pointer-events: none;
		position: absolute;
		width: 100%;
		height: 100%;
		content: '';
		-webkit-box-sizing: content-box; 
		-moz-box-sizing: content-box; 
		box-sizing: content-box;
	}
	.ficon:before {
		font-family:FontAwesome;
		speak: none;
		font-size: 24px;
		line-height: 40px;
		font-style: normal;
		font-weight: normal;
		font-variant: normal;
		text-transform: none;
		display: block;
		-webkit-font-smoothing: antialiased;
	}
	.icon-multi:before {content: "\f24d";}
	.icon-design:before {content: "\f108";}
	.icon-user:before {content: "\f0c0";}
	.icon-rocket:before {content: "\f135";}

	/* whatIDo icon */
	@-webkit-keyframes spinAround {
		from {-webkit-transform: rotate(0deg)}
		to {-webkit-transform: rotate(360deg);}
	}
	@-moz-keyframes spinAround {
		from {-moz-transform: rotate(0deg)}
		to {-moz-transform: rotate(360deg);}
	}
	@keyframes spinAround {
		from {transform: rotate(0deg)}
		to {transform: rotate(360deg);}
	}
	.doBox .icon_wrap .ficon {
		-webkit-transition: background 0.2s, color 0.2s;
		-moz-transition: background 0.2s, color 0.2s;
		transition: background 0.2s, color 0.2s;
		margin:0;
		color:#464e58;
	}
	.doBox .icon_wrap {background:#fff; margin-bottom:10px;}
	.doBox .icon_wrap:hover  {background:rgba(82, 149, 208,1);}

	.doBox p {font-size:1.125rem; color:#464e58}
	 .doBox .icon_wrap:hover .ficon,
		.doBox .icon_wrap:hover p {color:#fff;}
	.doBox .icon_wrap:hover .ficon:before {
		-webkit-animation: spinAround 2s linear infinite;
		-moz-animation: spinAround 2s linear infinite;
		animation: spinAround 2s linear infinite;
	}

