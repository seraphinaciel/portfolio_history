}
.carousel-control.left, .carousel-control.right {
	background:0
}
.carousel-control.left .fa, .carousel-control.right .fa { 
	position: absolute;
	top: 45%;
	font-size: 1.6em;
	color: #fff;
	text-shadow: 1px 1px 9px #777, 1px 1px 1px #555;
}
.carousel-control {
    text-shadow: none;
}
#view_contents {font-family:'NanumSquareR', 'nanumgothic'; line-height:28px;}
#view_contents em{color:#299ed9; text-transform:uppercase; font-style:normal; font:bold 14px 'Arial', sans-serif; display:block; padding-top: 27px; }
